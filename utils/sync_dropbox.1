#!/bin/bash
if [ "0" == `df ~/mydropbox/ | grep dbxfs  |wc -l` ]; then
	echo "Mounting"
	~/.local/bin/dbxfs ~/mydropbox/
	if [ "0" == `df ~/mydropbox/ | grep dbxfs  |wc -l` ]; then
		echo "mount failed";
		exit 1
	fi
fi
